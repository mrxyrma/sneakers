{
  "include": [
    "**/*.ts",
    "../src/**/*.cy.{js,jsx,ts,tsx",
    "./cypress.d.ts"
  ],
  "compilerOptions": {
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}